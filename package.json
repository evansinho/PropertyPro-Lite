{
  "name": "propertypro-lite",
  "version": "1.0.0",
  "description": "Property Pro Lite is a platform where people can create and/or search properties for sale or rent.",
  "main": "index.js",
  "scripts": {
    "test": "npm run migrations && nyc --reporter=text --reporter=html --require babel-register mocha ./server/test --timeout 2000 --exit",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "dev": "nodemon --exec babel-node ./server/index.js",
    "migrations": "babel-node ./server/config/migration.js --env",
    "clean": "rm -rf build",
    "build": "npm run clean && mkdir build && babel server -s -d build --ignore *.test.js",
    "heroku-postbuild": "cd server && npm install && npm run build",
    "start": "node build/index.js"
  },
  "engines": {
    "node": "8.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evansinho/PropertyPro-Lite.git"
  },
  "keywords": [
    "property",
    "agent"
  ],
  "author": "Evanson Igiri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evansinho/PropertyPro-Lite/issues"
  },
  "homepage": "https://github.com/evansinho/PropertyPro-Lite#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "joi-image-extension": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "nodemon": "^1.19.1",
    "pg": "^7.11.0",
    "prettier": "^1.13.0",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1"
  }
}
